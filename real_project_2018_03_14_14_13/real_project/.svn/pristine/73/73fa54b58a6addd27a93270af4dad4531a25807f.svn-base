	@CHARSET "UTF-8";
		
		.zeta-menu-bar {
		  border: 1px solid;
		  background: lightgreen;
		  display: inline-block;
		  
		  margin: 10px 10px;
		  width: 160px;
		}
		.zeta-menu { margin: 0; padding: 0; }
		.zeta-menu li {
		  float: none;
		  list-style:none;
		  position: relative;
		}
		/* .zeta-menu li:` { background: pink; } */
		.zeta-menu li.expand { background: white; }
		.zeta-menu li.expand>a { color: hotpink; }
		.zeta-menu a {
		  color: white;
		  display: block;
		  padding: 10px 20px;
		  text-decoration: none;
		}
		.zeta-menu ul {
		  background: #eee;
		  border: 1px solid silver;
		  display: none;
		  padding:10px 10px;
		  position: absolute;
		  left:100%;
		  top: 0%;
		  
		  width: 180px;
		  
		}
		.zeta-menu ul li { float: left; }
		.zeta-menu ul li.expand { background: #ddd; }
		.zeta-menu ul li.expand a{ color: black; }
		.zeta-menu ul a { color: black; }
		.zeta-menu ul ul { left: 100%; top: 0; }

	ul, li, ol{
		list-style-type: none;
	}
	#main-navbar {
	    position:fixed; 
	    top:0; 
	    left:0; 
	    right:0; 
	    z-index:10;
	    height:50px;
	    background-color: burlywood;
	}
	 #toggle-sidebar {
	     position:absolute; 
	     top:14px; 
	     right:20px; 
	     cursor:pointer;
	 }
	
	 #main-sidebar {
	     position:fixed; 
	     top:0; 
	     bottom:0;
	     background-color:white;
		display: none; 
	 }
	 #main-sidebar .close-sb {
	     padding:0 0 0 20px;
	     border-bottom:1px solid #c11b00;
	     font:20px/70px Acme;
	     cursor:pointer;
	 }
	
	 #main-sidebar ul li>a {
	     display:block;
	     padding:0 0 0 20px;
	     background-color:white;
	     border-top:1px solid #f4f4f4; 
	     border-bottom:1px solid #d4d4d4;
	     font:14px/48px NGBold; 
	     color:#555;
	 }
	
	 #main-sidebar ul li>a:hover {
	     background:#333;
	     border-top:1px solid; 
	     border-bottom:1px solid #010101;
	     color:#fff;
	 }
	 #main-sidebar ul li li>a {
	     display:block;
	     padding:0 0 0 20px;
	     background:#f7f7f7;
	     border-bottom:1px solid #dedee0;
	 }
	     
