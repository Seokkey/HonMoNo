package com.project.dto;

public class CategoryDto {
	private int c_idx;//카테고리 인덱스
	private String c_name;//카테고리 이름
	private String c_group;//대분류 구분
	private int num;//카테고리 분류번호
	private String mdgroup;//중분류 구분
	
	public int getC_idx() {
		return c_idx;
	}
	public void setC_idx(int c_idx) {
		this.c_idx = c_idx;
	}
	public String getC_name() {
		return c_name;
	}
	public void setC_name(String c_name) {
		this.c_name = c_name;
	}
	public String getC_group() {
		return c_group;
	}
	public void setC_group(String c_group) {
		this.c_group = c_group;
	}
	public int getNum() {
		return num;
	}
	public void setNum(int num) {
		this.num = num;
	}
	public String getMdgroup() {
		return mdgroup;
	}
	public void setMdgroup(String mdgroup) {
		this.mdgroup = mdgroup;
	}
	
}
