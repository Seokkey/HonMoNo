	#main-navbar {
	    position:fixed; 
	    top:0; 
	    left:0; 
	    right:0; 
	    z-index:10;
	    height:50px;
	    background-color: burlywood;
	}
	 #toggle-sidebar {
	     position:absolute; 
	     top:14px; 
	     right:20px; 
	     cursor:pointer;
	 }
	
	 #main-sidebar {
	     position:fixed; 
	     top:0; 
	     bottom:0;
	     background-color:white;
		display: none; 
	 }
	 #main-sidebar .close-sb {
	     padding:0 0 0 20px;
	     border-bottom:1px solid #c11b00;
	     font:20px/70px Acme;
	     cursor:pointer;
	 }
	
	 #main-sidebar ul li>a {
	     display:block;
	     padding:0 0 0 20px;
	     background-color:white;
	     border-top:1px solid #f4f4f4; 
	     border-bottom:1px solid #d4d4d4;
	     font:14px/48px NGBold; 
	     color:#555;
	 }
	
	 #main-sidebar ul li>a:hover {
	     background:#333;
	     border-top:1px solid; 
	     border-bottom:1px solid #010101;
	     color:#fff;
	 }
	 #main-sidebar ul li li>a {
	     display:block;
	     padding:0 0 0 20px;
	     background:#f7f7f7;
	     border-bottom:1px solid #dedee0;
	 }
	     
